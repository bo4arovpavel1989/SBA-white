<div class="row-fluid">
<div id="divHeaderLine1" style='margin-bottom: 40px;'>Пульс рынка</div>
	<div id='dataToLoad'>
		 <div class="span12">
                    <div id="gridArea">
                        <ul id="tiles" style='list-style-type: none;'>
						{{#each news}}
							<li style='background-image: url({{image}});background-size:contain;background-repeat: no-repeat;'>
								<h4>{{title}}</h4>
								<p>{{text}}</p>
							</li>
						{{/each}}
                         </ul>
                    </div>
		</div>	
		<div style='text-align:center'>
			<a href='#' class='btn btn-lg' id='moreNews'>Еще</a>
		 </div>
		<div class="loader"></div>		
	</div>
</div>
	<script src="/scripts/marketpulse.js" type="text/javascript"></script>
	<script>
		$(document).ready(function(){
			$('#marketLink').addClass('active');
		});
    </script>