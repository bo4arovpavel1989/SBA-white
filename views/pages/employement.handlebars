  <script src="https://cdn3.devexpress.com/jslib/17.1.4/js/dx.all.js"></script>
 <form class='form-horizontal' id='addToFilterGraph' action="/getemployementdata" method="post" style=' margin-left:40px; margin-top: 40px; ' enctype="multipart/form-data">
		<div id="divHeaderLine1" style='margin-bottom: 40px;'>Рынок труда</div>
		 <label>Период выборки</label>
					<select class="form-control" name='monthFrom' class='form-control'>
						<option value='01'>Январь</option>
						<option value='02'>Февраль</option>
						<option value='03'>Март</option>
						<option value='04'>Апрель</option>
						<option value='05'>Май</option>
						<option value='06'>Июнь</option>
						<option value='07'>Июль</option>
						<option value='08'>Август</option>
						<option value='09'>Сентябрь</option>
						<option value='10'>Октябрь</option>
						<option value='11'>Ноябрь</option>
						<option value='12'>Декабрь</option>
					</select>
					<select class="form-control" name='yearFrom' class='form-control'>
						<option value='2017'>2017</option>
					</select>
					<span class="secondDate"><b>&nbsp; - &nbsp;</b></span>
					<select class="form-control secondDate" name='monthTo' class='form-control'>
						<option value='01'>Январь</option>
						<option value='02'>Февраль</option>
						<option value='03'>Март</option>
						<option value='04'>Апрель</option>
						<option value='05'>Май</option>
						<option value='06'>Июнь</option>
						<option value='07'>Июль</option>
						<option value='08'>Август</option>
						<option value='09'>Сентябрь</option>
						<option value='10'>Октябрь</option>
						<option value='11'>Ноябрь</option>
						<option value='12'>Декабрь</option>
					</select>
					<select class="form-control  secondDate" name='yearTo' class='form-control'>
						<option value='2017'>2017</option>
					</select>
		 <label>Населенный пункт</label>
			<input type="text" name='city' placeholder='Выберите населенный пункт'></input>
		<label>Букмекерская контора</label>
			<select class="form-control" name='bk'>
				<label>Выберите БК</label>
				<option value=''>Все конторы</option>
				{{#each bk}}
				<option value='{{bk}}' data-name='{{name}}'>{{name}}</option>
				{{/each}}
			</select>
			<input class="btn btn-primary" type="submit" id='showGraph' value="Показать"></input>
			<a class="btn btn-danger" id='resetGraph'>Сброс</a>
	</form>	
	<div class="loader"></div>
	<div id='chart'>
	</div>
	<div id='dataToLoad'>
	</div>
	<script src="/scripts/employement.js" type="text/javascript"></script>
	 <script>
		$(document).ready(function(){
			$('#marketLink').addClass('active');
		});
    </script>